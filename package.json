{
  "name": "next-eslint-prettier-husky",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "commit-msg": "commitlint --edit ${1}",
    "commit": "git add . && git-cz",
    "dev": "next dev",
    "lint:fix": "next lint --fix",
    "lint": "next lint",
    "pre-commit": "yarn lint:fix",
    "prepare": "husky install",
    "prestart": "chmod ug+x .husky/* && chmod ug+x .git/hooks/*",
    "release:major": "standard-version --release-as major && git push --follow-tags",
    "release:minor": "standard-version --release-as minor && git push --follow-tags",
    "release:patch": "standard-version --release-as patch && git push --follow-tags",
    "start": "next start"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npx lint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/cz-commitlint": "^17.4.4",
    "@next/eslint-plugin-next": "^13.1.6",
    "@types/node": "18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "commitizen": "^4.3.0",
    "eslint": "8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "standard-version": "^9.5.0",
    "typescript": "4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
